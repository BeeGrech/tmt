/dmesg:
  test: /bin/true

  check: dmesg

/avc:
  check:
    - how: avc

  /harmless:
    test: /bin/true

  /nasty:
    test: |
      set -x
      systemctl status auditd || /bin/true
      sudo bash -c "passwd --help &> /root/passwd.log; \
                    ls -alZ /root/passwd.log; \
                    rm -f /root/passwd.log" || /bin/true
